<div class="teamchess-table" id="teamchess-matchlist-table">

<table>
<tr class="teamchess-tablehead">
	<th width="10%" class="t_center">&nbsp;</th>
	<th width="40%" class="t_center">Mot</th>
	<th width="15%" class="t_center">Datum</th>
	<th width="15%" class="t_center">Resultat</th>
	<th width="15%" class="t_center">Serie</th>
	<th width="5%" class="t_center">Lag</th>
</tr>

<!-- BEGIN MATCH -->
<tr class="teamchess-tablerow-{odd_even}">
	<td class="t_center">
		<a href="{list_url}&listtype=match&id={id}">{id}</a>
	</td>
	<td class="t_left">{opponent}</td>
	<td class="t_center">{date}</td>
	<td class="t_center">{result}</td>
	<td class="t_center">{league}</td>
	<td class="t_center">{team}</td>
</tr>
<!-- END MATCH   -->

</table>
</div>
