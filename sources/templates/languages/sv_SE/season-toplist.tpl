<div class="teamchess-table" id="teamchess-toplist-season-table">

<!-- BEGIN HEADER -->
<p class="teamchess-header">
Spelade matcher och poäng<br>
Säsongen {season}, {league}
</p>
<!-- END HEADER -->

<table>
<tr class="teamchess-tablehead">
	<th width="10%" class="t_center">&nbsp;</th>
	<th width="45%" class="t_left">Spelare</th>
	<th width="15%" class="t_center">Matcher</th>
	<th width="15%" class="t_center">Poäng</th>
</tr>

<!-- BEGIN NAME -->
<tr class="teamchess-tablerow-{odd_even}">
	<td class="t_center">{place}</td>
	<td class="t_left"><a href="{list_url}&listtype=player&id={id}">{player}</a></td>
	<td class="t_center">{number_of_matches}</td>
	<td class="t_center">{number_of_points}</td>
</tr>
<!-- END NAME   -->

</table>

</div>
