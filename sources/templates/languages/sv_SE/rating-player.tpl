<div class="teamchess-table" id="teamchess-rating-player-table">

<!-- BEGIN HEADER -->
<p class="teamchess-header">
	Rankinghistorik för {name}<br />
	Högsta ranking: {max}
</p>
<!-- END HEADER -->

<table>
<tr class="teamchess-tablehead">
	<th width="50%" class="t_center">Datum</th>
	<th width="50%" class="t_center">Ranking</th>
</tr>

<!-- BEGIN LIST -->
<tr class="teamchess-tablerow-{odd_even}">
	<td class="t_center">{date}</td>
	<td class="t_center">{rating}</td>
</tr>
<!-- END LIST   -->

</table>

</div>
